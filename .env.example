# GuardianChain Environment Configuration Template
# Copy this file to .env and fill in your actual values

# Backend Private Key (DO NOT COMMIT THE ACTUAL KEY)
BACKEND_PRIVATE_KEY=your_private_key_here

# Optional: Additional private keys for different roles
PRIVATE_KEY_REPORTER=your_reporter_private_key_here
PRIVATE_KEY_VERIFIER=your_verifier_private_key_here
PRIVATE_KEY_INVESTIGATOR=your_investigator_private_key_here

# Database Configuration (if using)
DATABASE_URL=your_database_url_here

# JWT Secret for API authentication
JWT_SECRET=your_jwt_secret_here

# Other sensitive configurations
API_SECRET_KEY=your_api_secret_here
ENCRYPTION_KEY=your_encryption_key_here
